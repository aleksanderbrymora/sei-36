<%= form_for @car do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :style %>
  <%= f.text_field :style %>
  <%= f.label :power %>
  <%= f.text_field :power %>
  <%= f.label :image %>
  <%= f.url_field :image, :required => true %>
  <%= f.label :brand_id %>
  <%= f.select :brand_id, Brand.pluck(:name, :id), :include_blank => true %>
  <%= f.submit %>
<% end %>
